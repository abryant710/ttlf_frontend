{
  "name": "ttfl-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "axios": "^0.21.1",
    "core-js": "^3.8.2",
    "i18next": "^19.8.2",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.3",
    "react-icons": "^3.11.0",
    "react-player": "^2.6.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-toastify": "^6.1.0",
    "react-tooltip": "^4.2.10",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "clean:yarn": "rm -rf node_modules || true && rm yarn.lock || true && yarn cache clean && yarn install",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "aws s3 sync build/ s3://ttlf-website --profile developer",
    "build_deploy": "npm run build && npm run deploy",
    "redeploy": "git push && npm run build_deploy",
    "lint": "eslint --fix . --ext .tsx,.ts ./src"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.13",
    "@types/lodash": "^4.14.162",
    "@types/node": "^14.10.0",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-toastify": "^4.1.0",
    "@types/react-tooltip": "^4.2.4",
    "airbnb": "0.0.2",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4"
  }
}
